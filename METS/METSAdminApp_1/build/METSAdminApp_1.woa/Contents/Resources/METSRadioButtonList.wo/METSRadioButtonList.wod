HasHelpText : WOConditional {
	condition = hasHelpText;
}

HelpText : WOString {
	value = ^helpText;
}

Label : WOGenericContainer {
	class = "control-label";
	elementName = "label";
	for = id;
}

LabelText : WOString {
	value = ^labelText;	
}

RadioButtonList : KMIRadioButtonList {
	displayString = ^displayString;
	item = ^item;
	list = ^list;
	selection = ^selection;
	wrapperElementName = "label";
	wrapperClass = "radio";
	value = ^value;
}