ContinueButton : WOSubmitButton {
	action = saveChanges;
	class = "btn";
	value = "Continue";
}

Main : PageWrapper {
	mainNavigationIndex = -1;
	metaDescription = metaDescription;
	pageTitle = pageTitle;
}

